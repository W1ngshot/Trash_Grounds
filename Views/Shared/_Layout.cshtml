        <link rel="stylesheet" href="~/lib/carbon-gray-100.min.css" />
        <link rel="stylesheet" href="~/css/_fonts.css"/>
        <link rel="stylesheet" href="~/css/_global.css" asp-append-version="true"/>
        <link rel="stylesheet" href="~/css/components/header.css" />
